    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".PlanejarActivity" android:label="Planejar" android:parentActivityName=".PlanejarActivity"/>
        <activity android:name=".DisciplinaActivity" android:label="Escolher Disciplina" android:parentActivityName=".DisciplinaActivity"/>
        <activity android:name=".ObjetivosAcitivity" android:label="Avaliar Objetivos" android:parentActivityName=".ObjetivosAcitivity"/>
        <activity android:name=".TempoActivity" android:label="Gerenciar Tempo" android:parentActivityName=".TempoActivity"/>
        <activity android:name=".TreinarActivity" android:label="Treinar" android:parentActivityName=".TreinarActivity"/>

    </application>

</manifest>